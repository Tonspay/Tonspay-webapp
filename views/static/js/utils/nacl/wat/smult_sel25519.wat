	(set_local $t (i64.and (get_local $c) (i64.xor (i64.load offset=88 (get_local $p)) (i64.load offset=88 (get_local $q)))))
	(i64.store offset=88 (get_local $p) (i64.xor (i64.load offset=88 (get_local $p)) (get_local $t)))
